@{
    ViewData["Title"] = "Home Page";

}

 <div class="content" data-simplebar>
			    <div class="section mt-0">
		            <h1 class="title title--h1 title__separate">Resume</h1>
				</div>
				
				<!-- Experience -->
				<div class="section">
					<h2 class="title title--h2"><img class="title-icon" src="/NewFolder2/assets/icons/icon-education.svg" alt="" /> Education</h2>
					<div class="timeline">
           @foreach (var education in Model.Educations)
            {
                <!-- Item -->
               
                    <h5 class="title title--h3 timeline__title"><i class="fa-regular fa-circle fa-beat" style="color: #0c78a3;"></i>
                        @education.EducationTitle
                    </h5>
                    <span class="timeline__period">
                        @education.EducationDuration
                    </span>
                    <p class="timeline__description">
                        @education.Description
                    </p>
           
            }
                                        
