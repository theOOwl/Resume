				<div class="section">
					<h2 class="title title--h2"><img class="title-icon" src="/NewFolder2/assets/icons/icon-experience.svg" alt="" /> Experience</h2>
					<div class="timeline">
            @foreach (var experience in Model.Experience)
            {
                <!-- Item -->
               
                    <h5 class="title title--h3 timeline__title"><i class="fa-regular fa-circle fa-beat" style="color: #f6972c;"></i>
                           @experience.ExperienceTitle
                    </h5>
                    <span class="timeline__period">
                        @experience.ExperienceDuration
                    </span>
                    <p class="timeline__description">
                        @experience.CompanySite
                    </p>
               
            }
					</div>
				</div>
						
				<!-- Skills -->
				<div class="section">
				    <h2 class="title title--h2">My Skills</h2>
				    <div class="box-gray">
           @foreach (var mySkill in Model.MySkills)
            {
                <!-- Progress -->
                <div class="progress">
                    <div class="progress-bar" role="progressbar" aria-valuenow="@mySkill.Percentage" aria-valuemin="0" aria-valuemax="100">
                        <div class="progress-text">
                            <span>
                                @mySkill.SkillTille
                            </span>
                            <span>
                                @mySkill.Percentage%
                            </span>
                        </div>
                    </div>
                    <div class="progress-text"><span>@mySkill.SkillTille</span></div>
                </div>
            }
                    </div>
				</div>
			</div><!-- Content End -->